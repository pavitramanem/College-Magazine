<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".login"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".register"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".admin"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".articles" />
        <activity
            android:name=".addArticle"
            android:parentActivityName=".articles" />
        <activity
            android:name=".viewArticle"
            android:parentActivityName=".articles" />
        <activity android:name=".adminView"
            android:parentActivityName=".admin"/>
        <activity android:name=".delete"
            android:parentActivityName=".admin"/>
    </application>

</manifest>